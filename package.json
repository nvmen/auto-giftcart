    "@stylistic/eslint-plugin": "4.2.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.15.3",
    "@types/pixelmatch": "5.2.6",
    "@types/pngjs": "6.0.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "allure-cucumberjs": "3.2.1",
